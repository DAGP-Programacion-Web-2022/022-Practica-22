<body>
    <h1>Numeros</h1>
    <p>La ruta /{n√∫mero} regresa si es par o impar</p>
    <% if(ID % 2 == 0){ %>
        <% for(var i = 0; i < ID*2; i++) {%>
            <li>ES PAR</li>
          <% } %>
      <% } else{ %>  
        <% for(var i = 0; i < ID; i++) {%>
            <li>ES IMPAR</li>
          <% } %>
     <% } %>
</body>
