<body>
    <form action="/updatePerson" method="post">
        <label for="objId">ID:</label>
        <input id="objId" name="objId" type="text" value="<%= `${person._id}` %>"><br><br>
        <label for="nss">NSS:</label>
        <input type="text" name="nss" id="nss" value="<%= `${person.nss}` %>"><br><br>
        <label for="nombre">Nombre:</label>
        <input type="text" name="nombre" id="nombre" value="<%= `${person.nombre}` %>"><br><br>
        <label for="edad">Edad:</label>
        <input type="text" name="edad" id="edad" value="<%= `${person.edad}` %>"><br><br>
        <label for="tipoSangre">Tipo Sangre:</label>
        <input type="text" name="tipoSangre" id="tipoSangre" value="<%= `${person.tipoSangre}` %>"><br><br>

        <input type="submit" value="Submit">
    </form>

</body>
</html>